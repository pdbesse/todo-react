  "bugs": {
    "url": "https://github.com/pdbesse/todo-react/issues"
  },
  "homepage": "https://github.com/pdbesse/todo-react#readme",
  "dependencies": {
    "concurrently": "^7.3.0",
    "dotenv": "^16.0.1",
    "eslint": "^7.12.1",
    "eslint-config-prettier": "^6.15.0",
    "prettier": "^2.1.2"
  }
}
